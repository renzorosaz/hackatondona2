<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pe.com.tintegro.sigs.mapper.EstadoOrdenMapper">

	<resultMap type="pe.com.tintegro.sigs.entidad.EstadoOrden" id="comboTypeResultMap">
		<result property="idEstado" column="id" />
		<result property="descripcionEstado" column="descripcion" />
	</resultMap>
	
	<select id="obtenerEstadoOrden" resultMap="comboTypeResultMap">
		SELECT * from "Main".fn_mw_select_tb_estado_orden();
	</select>
	
	<resultMap type="pe.com.tintegro.sigs.entidad.general.Confirmacion" id="confirmacionResultMap">
		<result property="id" column="id_result" />
		<result property="mensaje" column="no_respuesta" />
	</resultMap>
	
	<select id="updateEstadoOrden" resultMap="confirmacionResultMap">
		select * from "Main".fn_mw_update_estado_orden(#{jsonOrden});
	</select>



</mapper>