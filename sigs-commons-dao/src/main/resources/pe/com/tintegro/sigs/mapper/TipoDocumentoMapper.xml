<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pe.com.tintegro.sigs.mapper.TipoDocumentoMapper">

	<resultMap type="pe.com.tintegro.sigs.entidad.TipoDocumento" id="tipoDocumentoResultMap">
		<result property="idTipoDocumento" column="id" />
		<result property="nombreTipoDocumento" column="descripcion" />
	</resultMap>

	<select id="obtenerDocumento" resultMap="tipoDocumentoResultMap">
		SELECT * from "Main".fn_mw_select_tipo_documento();
	</select>

</mapper>